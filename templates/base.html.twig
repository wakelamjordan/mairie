<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>
            {% block title %}
                Welcome!
            {% endblock %}
        </title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text><text y=%221.3em%22 x=%220.2em%22 font-size=%2276%22 fill=%22%23fff%22>sf</text></svg>">
        {% include './_part/link.html.twig' %}
        {% block stylesheets %}{% endblock %}
        {{ encore_entry_link_tags('app') }}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            {% block importmap %}
                {{ importmap('app') }}
            {% endblock %}
        {% endblock %}
    </head>
    <body class="bg-page">
        <div class="position-relative">
            <div class="mx-auto w-100 container px-3 px-md-4 px-lg-5 bg-page">
                {% include './_part/header.html.twig' %}
            </div>
            <div class="bg-60 container px-0">

                <div class="mx-auto w-100  position-sticky top-0">

                    {% include './_part/navbar.html.twig' %}
                </div>

                <div class="mx-auto w-100 px-3 px-md-4 px-lg-5 bg-60">

                    <div class="min-vh-100">

                        {% block body %}{% endblock %}
                    </div>
                </div>
            </div>
            {% include './_part/footer.html.twig' %}
        </div>

    </body>
</html>
